{
  "name": "isomorphic-boilerplate",
  "version": "1.0.1",
  "description": "isomorphic webapp boilerplate",
  "main": "app.js",
  "scripts": {
    "start": "NODE_ENV=production node app.js",
    "test": "npm run test:client && npm run test:server",
    "test:client": "nva test --config client/test/unit/fixture/conf.js",
    "test:e2e": "nva-test-e2e  -r client/test/e2e/fixture/server.js -c client/test/e2e/fixture/conf.js",
    "test:server": "mocha --require babel-register server/test/spec/**/*.js",
    "lint:js": "eslint client server",
    "lint:css": "stylelint \"client/bundle/**/*.{css,less,scss,styl}\"",
    "lint": "npm run lint:js && npm run lint:css"
  },
  "author": "alichen",
  "repository": "https://github.com/ali322/isomorphic-boilerplate",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.15.3",
    "chai": "^3.5.0",
    "classnames": "^2.2.5",
    "font-awesome": "^4.7.0",
    "jsdom": "^9.12.0",
    "koa": "2",
    "koa-body": "^2.0.1",
    "koa-methodoverride": "^2.0.0",
    "koa-router": "^7.1.0",
    "koa-session-minimal": "^3.0.3",
    "koa-static": "^3.0.0",
    "koa-views": "^5.2.0",
    "lru-cache": "^4.0.2",
    "normalize.css": "^7.0.0",
    "swig": "^1.4.2",
    "vue": "^2.2.2",
    "vue-server-renderer": "^2.2.2",
    "vuex": "^2.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-env": "^1.4.0",
    "browser-env": "^2.0.31",
    "eslint": "^3.15.0",
    "eslint-plugin-css-modules": "^2.2.0",
    "eslint-plugin-import": "^2.2.0",
    "mocha": "^3.2.0",
    "moxios": "^0.3.0",
    "nva-server": "^0.3.59",
    "nva-test": "^0.3.59",
    "sinon": "^2.0.0-pre.6",
    "sinon-chai": "^2.8.0",
    "stylelint": "^7.8.0",
    "stylelint-config-standard": "^16.0.0",
    "supertest": "^3.0.0",
    "vue-loader": "^11.1.4",
    "vue-template-compiler": "^2.2.3"
  }
}
