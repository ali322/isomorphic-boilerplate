{
  "name": "hyena",
  "version": "1.0.0",
  "description": "isomorphic webapp boilerplate",
  "main": "app.js",
  "scripts": {
    "linter": "esw -w --ext .js,.jsx,.es6 .",
    "test": "NODE_ENV=test ava --tap | tap-spec"
  },
  "ava": {
    "files": [
      "__tests__/client/**/*.js",
      "__tests__/server/**/*.js"
    ],
    "failFast": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "keywords": [
    "isomorphic boilerplate"
  ],
  "author": "alichen",
  "repository": "https://github.com/ali322/isomorphic-boilerplate",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.15.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "classnames": "^2.2.5",
    "font-awesome": "^4.7.0",
    "isomorphic-fetch": "^2.2.1",
    "koa": "2",
    "koa-bodyparser": "^2.3.0",
    "koa-convert": "^1.2.0",
    "koa-generic-session": "^1.11.4",
    "koa-methodoverride": "^2.0.0",
    "koa-router": "^7.1.0",
    "koa-static": "^2.0.0",
    "koa-views": "^5.2.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-container": "^1.0.16",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0",
    "swig": "^1.4.2",
    "xhr2": "^0.1.4"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-polyfill": "^6.20.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-optimize": "^1.0.1",
    "react-hot-loader": "^1.3.1"
  }
}
